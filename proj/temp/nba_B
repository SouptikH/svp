digraph {
	rankdir=LR
	"" [shape=none]
	p3 [shape=doublecircle style=solid]
	p0 [shape=circle style=bold]
	p2 [shape=circle style=solid]
	p1 [shape=doublecircle style=solid]
	"" -> p0
	p2 -> p3 [label=x]
	p0 -> p1 [label=x]
	p2 -> p2 [label=x]
	p3 -> p0 [label=y]
	p1 -> p1 [label=y]
	p1 -> p2 [label=y]
}
